    "url": "https://github.com/seattleio/boundaries/issues"
  },
  "homepage": "https://github.com/seattleio/boundaries#readme",
  "dependencies": {
    "choo": "^4.0.0-5",
    "dom-css": "^2.1.0",
    "insert-css": "^1.0.0",
    "mapbox": "^1.0.0-beta4",
    "mapbox.js": "^3.0.0",
    "qs": "^6.3.0",
    "request": "^2.78.0",
    "seattle-boundaries": "^5.0.0",
    "turf": "^3.0.14",
    "xhr": "^2.2.2",
    "xtend": "^4.0.1"
  },
  "devDependencies": {
    "browserify": "^13.1.1",
